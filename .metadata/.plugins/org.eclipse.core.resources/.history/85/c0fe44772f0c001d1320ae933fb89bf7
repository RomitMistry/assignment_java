package numberdigit;

public class numbers {
	public static void main(String[] args) {
		
			Scanner sc = new Scanner(System.in);
			System.out.print("ENter Number : ");
			int a = sc.nextInt();
			int count =0;
			
			while(a!=0) {
				a=a/10;
				count++;
			}
			System.out.println("Count of digit is :" +count);
			
			
		}
	}


